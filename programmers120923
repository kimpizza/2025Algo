class Solution {
    public int[] solution(int num, int total) {
        int [] answer = new int[num];
        
        for (int i = 0; i < answer.length; i++) {
            int sum = (num - 1) * num / 2;
            System.out.println(sum);
            answer[i] = (total - sum) / num + i;
            System.out.println(answer[i]);

        }
        
        return answer;
    }
}
